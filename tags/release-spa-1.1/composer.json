{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",

    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "illuminate/html": "5.0.*",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "guzzlehttp/guzzle": "~6.0",
        "moltin/laravel-cart": "dev-master",
        "cartalyst/sentinel": "2.0.*",
        "sammyk/laravel-facebook-sdk": "~3.0",
        "intervention/image": "~2.0",
        "sofa/eloquence": "~5.1@dev",
        "yajra/laravel-datatables-oracle": "^6.1",
        "barryvdh/laravel-dompdf": "0.6.*",
        "barryvdh/laravel-ide-helper": "2.1.*",
        "fzaninotto/company-name-generator": "dev-master",
        "mailgun/mailgun-php": "~2.0",
        "php-http/laravel-httplug": "*",
        "php-http/guzzle6-adapter": "^1.1",
        "kryptonit3/counter": "5.2.*",
        "simplesoftwareio/simple-qrcode": "~1",
        "barryvdh/laravel-snappy": "^0.3.1",
        "milon/barcode": "^5.1",
        "teamtnt/tntsearch": "^1.0",
        "tymon/jwt-auth": "0.5.*",
        "doctrine/dbal": "*",
        "symfony/psr-http-message-bridge": "^1.0",
        "mews/captcha": "dev-master",
        "laravel/socialite": "^2.0",
        "phpwhois/phpwhois": "~4.0",
        "maatwebsite/excel": "~2.1.0",
        "brozot/laravel-fcm": "^1.2",
        "mike42/escpos-php": "^2.0"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php",
            "app/Helper.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/myTestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																								
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}